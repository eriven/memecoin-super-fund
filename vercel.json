{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/js/(.*)",
            "dest": "/static/js/$1",
            "headers": { "Content-Type": "application/javascript" }
        },
        {
            "src": "/static/css/(.*)",
            "dest": "/static/css/$1",
            "headers": { "Content-Type": "text/css" }
        },
        {
            "src": "/static/images/(.*)",
            "dest": "/static/images/$1",
            "headers": { "Content-Type": "image/svg+xml" }
        },
        {
            "src": "/static/sounds/(.*)",
            "dest": "/static/sounds/$1",
            "headers": { "Content-Type": "application/javascript" }
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
